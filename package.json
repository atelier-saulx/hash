{
  "name": "@saulx/hash",
  "version": "3.0.0",
  "description": "Fast, low collision hashing based on djb2",
  "repository": "https://github.com/atelier-saulx/hash",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "ava --verbose"
  },
  "type": "module",
  "main": "./dist/index.js",
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "files": [
      "test/**/*.ts"
    ]
  },
  "keywords": [
    "hash",
    "performance",
    "object"
  ],
  "prettier": "@saulx/prettier-config",
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "ava": "5.3.1",
    "ts-node": "10.9.1",
    "@saulx/prettier-config": "^1.0.0"
  }
}
